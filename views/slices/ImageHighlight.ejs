<section class="highlight content-section">
  <div class="highlight-left">
    <%- ctx.prismicH.asHTML(slice.primary.title) %> 
    <%- ctx.prismicH.asHTML(slice.primary.headline) %>
    <p>
      <a href="<%- ctx.prismicH.asLink(slice.primary.link) %>">
        <%- ctx.prismicH.asText(slice.primary.link_label) %>
      </a>
    </p>
  </div>
  <div class="highlight-right">
    <img 
      src="<%- slice.primary.featured_image.url %>"
      alt="<%- slice.primary.featured_image.alt %>"
    />
  </div>
</section>
